{
  "name": "homyz-real-estate-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "üè° Homyz - Modern Real Estate Mobile App built with React Native and Expo",
  "keywords": ["react-native", "expo", "real-estate", "mobile-app", "typescript", "appwrite"],
  "author": "Mirna Badr <mirnabadr33@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/mirnabadr/homyz-real-estate-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mirnabadr/homyz-real-estate-app.git"
  },
  "bugs": {
    "url": "https://github.com/mirnabadr/homyz-real-estate-app/issues"
  },
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "clear": "npx expo start --clear",
    "reset": "npx expo r -c",
    "clean": "rm -rf node_modules/.cache && rm -rf .expo && npx expo start --clear",
    "fix-metro": "npx react-native start --reset-cache",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "seed": "tsx lib/seed.ts",
    "prebuild": "expo prebuild",
    "eject": "expo eject",
    "deploy": "expo publish",
    "doctor": "expo doctor",
    "install:clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.10",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-appwrite": "^0.14.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "use-debounce": "^10.0.6"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
